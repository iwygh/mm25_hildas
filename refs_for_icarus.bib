@incollection{Morbidelli:2020,
title = "Chapter 2 - Kuiper belt: Formation and evolution",
editor = "Dina Prialnik and M. Antonietta Barucci and Leslie A. Young",
booktitle = "The Trans-Neptunian Solar System",
publisher = "Elsevier",
pages = "25 - 59",
year = "2020",
isbn = "978-0-12-816490-7",
doi = "https://doi.org/10.1016/B978-0-12-816490-7.00002-3",
url = "http://www.sciencedirect.com/science/article/pii/B9780128164907000023",
author = "Alessandro Morbidelli and David Nesvorn{\'y}",
keywords = "Kuiper belt, Cold population, Hot population, Resonant population, Scattered disk, Fossilized/detached population, Solar system: Origin, Planetesimal formation, Planetesimal collisional evolution, Streaming instability, Trojans, Irregular satellites, Oort cloud, D-type asteroids",
abstract = "This chapter reviews accretion models for Kuiper belt objects (KBOs), discussing in particular the compatibility of the observed properties of the KBO population with the streaming instability paradigm. Then it discusses how the dynamical structure of the KBO population, including the formation of its five subcomponents (cold, hot, resonant, scattered, and fossilized), can be quantitatively understood in the framework of the giant planet instability. We also establish the connections between the KBO population and the Trojans of Jupiter and Neptune, the irregular satellites of all giant planets, the Oort cloud and the D-type main belt asteroids. Finally, we discuss the collisional evolution of the KBO population, arguing that the current size-frequency distribution below 100km in size has been achieved as a collisional equilibrium in a few tens of Myr inside the original massive Trans-Neptunian disk, possibly with the exception of the cold population subcomponent."
}

